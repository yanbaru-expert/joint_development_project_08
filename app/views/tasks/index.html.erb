<%= link_to "投稿画面へ", new_task_path %>
<% @tasks.each do |task| %>
<p>
    <%= task.title %>
    <%= link_to "詳細", task_path(task.id)%>
    <%= link_to "編集", "/tasks/#{task.id}/edit", method: :get %>
    <%= link_to "削除", task_path(task), method: :delete, data: {confirm: "削除しますか？"} %>   
</p>
<% end %>